set complete=search,location

map , <Leader>

nmap <C-j> <Nop>

nmap <Leader>E :sil !tvim --floating ~/.pentadactylrc &<CR>
nmap <Leader>R -js dactyl.windows.forEach(function(win) {win.dactyl.modules.io.source('~/.pentadactylrc')});
nmap -builtin <C-s> <C-u>

map c b

nmap -builtin <C-a> gT
nmap -builtin <C-f> gt
nmap -builtin <C-h> gT
nmap -builtin <C-l> gt

imap -builtin <C-a> <Esc>gT
imap -builtin <C-f> <Esc>gt

imap -builtin <C-h> <C-Left>
imap -builtin <C-l> <C-Right>

cmap -builtin <C-h> <C-Left>
cmap -builtin <C-j> <Down>
cmap -builtin <C-k> <Up>
cmap -builtin <C-l> <C-Right>

nmap -builtin A H
nmap -builtin S F
nmap -builtin F L

map <C-q> <Nop>

map gT <Nop>
map gt <Nop>

map H <Nop>
map L <Nop>

nmap -builtin ZQ ZZ
